<script>

    $(function() {
        var sidebar = $('.topbar');
        var top = sidebar.offset().top - parseFloat(sidebar.css('margin-top'));

        $(window).scroll(function(event) {
            var y = $(this).scrollTop();
            if (y >= top) {
                sidebar.addClass('fixed');
            } else {
                sidebar.removeClass('fixed');
            }
        });
    });


</script>
<script type="text/javascript">
    $('.clockpicker').clockpicker()
            .find('input').change(function() {
        console.log(this.value);
    });
    var input = $('#single-input').clockpicker({
        placement: 'bottom',
        align: 'left',
        autoclose: true,
        'default': 'now'
    });

// Manually toggle to the minutes view
    $('#check-minutes').click(function(e) {
        // Have to stop propagation here
        e.stopPropagation();
        input.clockpicker('show')
                .clockpicker('toggleView', 'minutes');
    });
    if (/mobile/i.test(navigator.userAgent)) {
        $('input').prop('readOnly', true);
    }
</script>

<script>
    $(document).ready(function() {

        $("#outstation_from").validate({
            rules: {
                outstation_from: {
                    required: true
                }
            },
            messages: {
                outstation_from: {
                    required: "Please Select Source"
                }
            },
            submitHandler: function(form) {
                form.submit();
            }
        });

        $("#pickup_now").click(function() {
            $("#datetime").hide();
        });
        $("#pickup_later").click(function() {
            $("#datetime").show();
        });
        
$('#multy_Outstation').find('input,select').attr('disabled', 'disabled');
        $("#roundtrip").click(function() {
            $("#round_trip").show();
            $("#multy_Outstation").hide();
            $("#roundtrip_Outstation").show();
            $('#roundtrip_Outstation').find('input,select').attr('disabled', false);
            $('#multy_Outstation').find('input,select').attr('disabled', 'disabled');
            //$("#one_way").hide();
        });
        $("#oneway").click(function() {
            $("#round_trip").hide();
            $("#roundtrip_Outstation").show();
            $("#multy_Outstation").hide();
            $('#roundtrip_Outstation').find('input,select').attr('disabled', false);
            $('#multy_Outstation').find('input,select').attr('disabled', 'disabled');
            //$("#one_way").show();
        });

        $("#multicity").click(function() {
            $("#roundtrip_Outstation").hide();
            $("#round_trip").hide();
            $("#multy_Outstation").show();
            $('#roundtrip_Outstation').find('input,select').attr('disabled', 'disabled');
            $('#multy_Outstation').find('input,select').attr('disabled', false);
        });


    });
</script>

